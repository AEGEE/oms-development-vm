# Objectclasses
objectclass ( 1.3.6.1.4.1.19881.1.3.1 NAME 'aegeePerson'
	DESC 'AEGEE person'
	SUP person STRUCTURAL
	MAY ( csn $ bodyCode $ gender $ yearOfBirth $ monthOfBirth $ dayOfBirth ) )

objectclass ( 1.3.6.1.4.1.19881.1.3.2 NAME 'aegeeBody'
	DESC 'AEGEE body'
	SUP organization STRUCTURAL
	MUST bodyCode
	MAY ( netcomCode $ bodyStatus $ bodyCategory $ exportPassword ) )

objectclass ( 1.3.6.1.4.1.19881.1.3.3 NAME 'omsBody'
	DESC 'AEGEE OMS body'
	SUP top STRUCTURAL
	MUST ( bodyCode $ bodyName $ bodyStatus $ bodyCategory $ bodyCategoryOrder $ mail $ lastBoardElectionDate $ foundedDate $ active )
	MAY ( bodyNameAscii $ netcom $ labeledURI $ latitude $ longitude $ careOf $ street $ postalCode $ localityName $ c $ telephoneNumber $ fax $ sipNumber $ officeLocation $ officeHours $ socialMeetingLocation $ socialMeetingHours $ info $ changeLog ) )

objectclass ( 1.3.6.1.4.1.19881.1.3.4 NAME 'omsAccount'
	DESC 'AEGEE OMS account'
	SUP account STRUCTURAL
	MUST ( givenName $ mail $ gender $ birthDate $ active )
	MAY ( userPassword $ csn $ preferredLanguage $ fieldOfStudies $ loginCount $ lastLoginDate $ lastLoginIP $ changeLog ) )

objectclass ( 1.3.6.1.4.1.19881.1.3.5 NAME 'omsGroup'
	DESC 'AEGEE OMS group'
	SUP top STRUCTURAL
	MUST ( cn )
	MAY ( memberUid $ changeLog ) )

objectclass ( 1.3.6.1.4.1.19881.1.3.6 NAME 'omsMember'
	DESC 'AEGEE OMS member'
	SUP person STRUCTURAL
	MUST ( bodyCode $ givenName )
	MAY ( title $ careOf $ street $ postalCode $ localityName $ c $ telephoneNumber $ mobileNumber $ fax $ homeCareOf $ homeStreet $ homePostalCode $ homeLocalityName $ homeCountryName $ homePhone $ homeFax $ sipNumber $ mail $ memberSinceDate $ memberUntilDate $ memberType $ jpegPhoto $ iban $ bic $ bankaccount $ messenger $ labeledURI $ studentType $ university $ info $ changeLog ) )
